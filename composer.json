{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "ext-pdo": "*",
        "bensampo/laravel-enum": "^6.3.1",
        "filament/filament": "^2.17.15",
        "guzzlehttp/guzzle": "^7.5",
        "laravel/framework": "^10.3.3",
        "laravel/sanctum": "^3.2.1",
        "laravel/tinker": "^2.8.1",
        "livewire/livewire": "^2.12.3",
        "ralphjsmit/laravel-seo": "^1.3",
        "spatie/laravel-markdown": "^2.2.6",
        "spatie/laravel-sluggable": "^3.4.2"
    },
    "require-dev": {
        "doctrine/dbal": "^3.6.1",
        "fakerphp/faker": "^1.21.0",
        "laravel/pint": "^1.6",
        "laravel/sail": "^1.21.2",
        "mockery/mockery": "^1.5.1",
        "nunomaduro/collision": "^7.1",
        "phpunit/phpunit": "^10.0.15",
        "spatie/laravel-ignition": "^2.0",
        "tightenco/duster": "^1.0.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "@php artisan filament:upgrade"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "lint": "duster lint",
        "fix": "duster fix"
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
